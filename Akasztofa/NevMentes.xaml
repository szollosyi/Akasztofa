<Window x:Class="Akasztofa.NevMentes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Akasztofa"
        mc:Ignorable="d"
        Title="NevMentes" Height="300" Width="300">
    <StackPanel>
        <Label Content="Felhasználónév" Margin="5"/>
        <TextBox x:Name="tbxFelhasználónév" Margin="5"/>
        <Button Content="Mentés" Margin="5" Click="Button_Click"/>
    </StackPanel>
</Window>
